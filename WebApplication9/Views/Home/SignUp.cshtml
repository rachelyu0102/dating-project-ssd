@model WebApplication9.Models.SignUp

@{
    ViewBag.Title = "SignUp";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}


<p class="col-md-4 col-md-offset-4 error">@Html.Raw(ViewBag.ErrorMessage)</p>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <form action="MyAccount">
        <div class="form-horizontal col-md-4 col-md-offset-4">
            <div class="page-header">
                <h4>Join Dating Today.</h4>
            </div>

            <input class="col-md-10 col-md-offset-1" type="text" placeholder="Username">
            <input class="col-md-10 col-md-offset-1" type="text" placeholder="Email">
            <input class="col-md-10 col-md-offset-1" type="text" placeholder="password">


            <div class="form-group">
                <div class="col-md-10 col-md-offset-1 ">
                    <input type="submit" value="Sign up" class="btn btn-info col-md-12" />
                </div>
            </div>
            <p class="col-md-10 col-md-offset-2">Have an account? <a href="~/Home">Log in</a></p>

        </div>
    </form>





}


<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
