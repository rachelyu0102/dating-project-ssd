
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/HomePage.cshtml";
}

<form action="~/Home/MyAccount" class="col-md-3 col-md-offset-8">
    <div class="page-header">
        <h4>Welcome Back</h4>
    </div>
    <div class="form-group" style="margin-bottom:0px">
        <label for="InputEmail1"></label>
        <input type="email" class="form-control" id="InputEmail1" placeholder="Email">
    </div>
    <div class="form-group">
        <label for="InputPassword"></label>
        <input type="password" class="form-control" id="InputPassword" placeholder="Password">
    </div>
    <button type="submit" class="btn btn-info col-md-12">Log in </button>
    <p class="col-md-10 col-md-offset-2">Not a member? <a href="~/Home/SignUp">Join now</a></p>
</form>


<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

